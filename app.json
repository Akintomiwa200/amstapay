{
  "expo": {
    "name": "amstapay",
    "slug": "amstapay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash.jpeg",
    "scheme": "amstapay",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.herkintormiwer.amstapay"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash.jpeg",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.herkintormiwer.amstapay"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/splash.jpeg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.jpeg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1ff1fbf8-609e-4ffd-8020-c4c0d021bf3f"
      }
    }
  }
}
